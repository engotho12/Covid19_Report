  <DataSets>
    <DataSet Name="Sub_DSet">
      <Query>
        <DataSourceName>Sub_DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StateName">
            <Value>=Parameters!StateName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        County, StateName, Sum(Cases) as TotCases, Sum(Deaths) as TotDeaths
FROM            Covid_Cases
Where StateName = @StateName
Group By County, StateName</CommandText>
      </Query>
